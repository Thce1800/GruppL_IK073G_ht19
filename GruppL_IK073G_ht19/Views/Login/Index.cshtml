
@{
    ViewBag.Title = "Login";
    Layout = null;
    <link rel="stylesheet" type="text/css" href="~/Content/Login.css">
    <link href="https://fonts.googleapis.com/css?family=Inconsolata&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Sedgwick+Ave+Display&display=swap" rel="stylesheet">
}

<h1 class="ikon">IT for U</h1>



<form id="login">
    <h1 id="loginRubrik">Logga in</h1>
    <div>

        <label>
            <span>Användarnamn *</span>
            <input id="Användarnamn" placeholder="Skriv ditt användarnamn.." type="text" tabindex="1" autofocus>
        </label>

    </div>

    <div>

        <label>
            <span>Lösenord *</span>
            <input id="password" placeholder="Skriv ditt lösenord.." type="password" tabindex="2">
        </label>

    </div>

    <div class="buttons">
        <a href="/Persons/Index" id="button" class="button1" type="button" tabindex="3">Kund</a>

        <a href="/Customers/Index" id="button" class="button1" tabindex="4">Frilansare</a>
    </div>

    <div class="clear"></div>

    <div class="new">
        <a href="/Customers/Create" id="new1" type="button" tabindex="5">Ny kund?</a>
        <a href="/Persons/Create" id="new2" type="button" tabindex="6">Ny frilansare?</a>
    </div>



</form>

