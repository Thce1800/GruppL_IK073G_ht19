@model IEnumerable<GruppL_IK073G_ht19.Models.Competences>

@{
    ViewBag.Title = "Index";
}

<h2>Kompetenser</h2>

<a href="/Competences/Create"><button class="logout"><b>+</b>Lägg till</button></a>

<table class="table">
    <tr>
        <th>
            Kompetens
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Competence)
            </td>
            <td>
                @Html.ActionLink("Redigera", "Edit", new { id = item.Competence_id }) |
                @Html.ActionLink("Radera", "Delete", new { id = item.Competence_id })
            </td>
        </tr>
    }

</table>
<a href="/Persons/Details/1"><button class="logout">Tillbaka till profilen</button></a>
