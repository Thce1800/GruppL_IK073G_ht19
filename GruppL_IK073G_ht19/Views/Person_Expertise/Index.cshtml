@model IEnumerable<GruppL_IK073G_ht19.ViewModels.PECViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Kompetenser och Expertiser</h2>

<p>
    @Html.ActionLink("Skapa ny", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Kompetens
        </th>
        <th>
            Expertis
        </th>
        <th>
            Gradering
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Competence)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Expertise)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Grade)
            </td>
            <td>
                @Html.ActionLink("Radera", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>

<a href="/Persons/Details/1"><button class="logout">Tillbaka till profilen</button></a>

