@model IEnumerable<GruppL_IK073G_ht19.ViewModels.PersonExpertiseCompetensViewModel>


@{
    ViewBag.Title = "Index";
}

<h2>Expertiser</h2>

<a href="/Person_Expertise/Create"><button class="logout"><b>+</b>Lägg till</button></a>


<table class="table">
    <tr>
        <th>
            Kompetens
        </th>
        <th>
            Expertis
        </th>
        <th>
            Gradering
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Competence)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Expertise)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Grade)
            </td>
            <td>
                @Html.ActionLink("Redigera", "Edit", new { id = item.Expertise_id }) |
                @Html.ActionLink("Radera", "Delete", new { id = item.Expertise_id })
            </td>
        </tr>
    }

</table>
<a href="/Persons/Details/1"><button class="logout">Tillbaka till profilen</button></a>
<a href="/Competences/Index/1"><button class="logout">Redigera kärnförmågor</button></a>
